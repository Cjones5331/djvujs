{
  "name": "djvu.js_website",
  "scripts": {
    "start": "npx @11ty/eleventy --serve --port 9000",
    "css": "sass --no-source-map --watch src/css/main.scss src/css/main.css",
    "build:css": "sass --no-source-map src/css/main.scss src/css/main.css",
    "build:html": "npx @11ty/eleventy",
    "build": "node .js delete && npm run build:css && npm run build:html && node .js cname",
    "copy": "node .js copy",
    "make": "npm i && npm run copy && npm run build",
    "static": "npx serve docs -l 9000"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "jstransformer-markdown-it": "^2.1.0",
    "sass": "^1.39.0",
    "fs-extra": "^10.0.0"
  }
}
